@model ISKI.SARS.WebUI.Models.TagViewModel
@{
    ViewData["Title"] = "Edit Tag";
}
<h2>Edit Tag</h2>
<form asp-action="Edit" method="post">
    <input type="hidden" asp-for="Id" />
    <input type="hidden" asp-for="ReportTemplateId" />
    <div class="mb-3">
        <label asp-for="TagName" class="form-label"></label>
        <input asp-for="TagName" class="form-control" />
        <span asp-validation-for="TagName" class="text-danger"></span>
    </div>
    <div class="mb-3">
        <label asp-for="TagNodeId" class="form-label"></label>
        <input asp-for="TagNodeId" class="form-control" />
        <span asp-validation-for="TagNodeId" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
    <a asp-action="Index" asp-route-templateId="@Model.ReportTemplateId" class="btn btn-secondary">Cancel</a>
</form>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
