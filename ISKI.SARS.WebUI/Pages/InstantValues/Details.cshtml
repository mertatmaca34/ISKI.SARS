@page
@model ISKI.SARS.WebUI.Pages.InstantValues.DetailsModel
@{
    ViewData["Title"] = "Instant Value Details";
}
<h1>Instant Value Details</h1>
<div>
    <dl class="row">
        <dt class="col-sm-2">Timestamp</dt>
        <dd class="col-sm-10">@Model.Value.Timestamp</dd>
        <dt class="col-sm-2">TagId</dt>
        <dd class="col-sm-10">@Model.Value.ReportTemplateTagId</dd>
        <dt class="col-sm-2">Value</dt>
        <dd class="col-sm-10">@Model.Value.Value</dd>
        <dt class="col-sm-2">Status</dt>
        <dd class="col-sm-10">@Model.Value.Status</dd>
    </dl>
</div>
