@page
@model ISKI.SARS.WebUI.Pages.InstantValues.EditModel
@{
    ViewData["Title"] = "Edit Instant Value";
}
<h1>Edit Instant Value</h1>
<form method="post">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="mb-3">
        <label asp-for="Value.ReportTemplateTagId" class="form-label"></label>
        <input asp-for="Value.ReportTemplateTagId" class="form-control" />
        <span asp-validation-for="Value.ReportTemplateTagId" class="text-danger"></span>
    </div>
    <div class="mb-3">
        <label asp-for="Value.Value" class="form-label"></label>
        <input asp-for="Value.Value" class="form-control" />
        <span asp-validation-for="Value.Value" class="text-danger"></span>
    </div>
    <div class="mb-3">
        <label asp-for="Value.Timestamp" class="form-label"></label>
        <input asp-for="Value.Timestamp" class="form-control" />
        <span asp-validation-for="Value.Timestamp" class="text-danger"></span>
    </div>
    <div class="mb-3 form-check">
        <input asp-for="Value.Status" class="form-check-input" />
        <label asp-for="Value.Status" class="form-check-label"></label>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
</form>
